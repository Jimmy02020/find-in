{
  "name": "find-in",
  "version": "0.1.0",
  "description": "Searching Text In File For Node.",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "mocha  --compilers js:babel-core/register --recursive test",
    "test:general": "mocha  --compilers js:babel-core/register test/general.test.js -w",
    "test:longReg": "mocha  --compilers js:babel-core/register test/longReg.test.js -w",
    "test:chunkBoundaries": "mocha  --compilers js:babel-core/register test/chunkBoundaries.test.js -w",
    "test:cov": "istanbul cover node_modules/mocha/bin/_mocha --  --compilers js:babel-core/register test",
    "check:cov": "istanbul check-coverage --root coverage --statement 90 --branch 75 --function 80 --line 95",
    "report:cov": "cat ./coverage/lcov.info | codecov",
    "prebuild": "rimraf lib",
    "build": "babel --out-dir lib src",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jimmy02020/find-in.git"
  },
  "keywords": [
    "fs",
    "file",
    "find",
    "stream",
    "finder"
  ],
  "files": [
    "lib",
    "README.md"
  ],
  "author": "'Jalal Maskoun<'jimmy002020@gmail.com'>",
  "license": "'MIT'",
  "bugs": {
    "url": "https://github.com/Jimmy02020/find-in/issues"
  },
  "homepage": "https://github.com/Jimmy02020/find-in#readme",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-preset-es2015": "6.24.1",
    "chai": "4.1.0",
    "codecov.io": "0.1.6",
    "eslint": "4.3.0",
    "eslint-config-airbnb-base": "11.3.1",
    "eslint-plugin-import": "2.7.0",
    "faker": "4.1.0",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "3.5.0",
    "rimraf": "2.6.1"
  }
}
